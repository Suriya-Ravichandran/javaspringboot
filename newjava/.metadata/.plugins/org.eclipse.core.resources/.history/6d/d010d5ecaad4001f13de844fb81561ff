package muthu;
abstract class demo{
	void print()
	{
		System.out.println("hello");
		
	}
	abstract void add();
}
class sample1 extends demo
{
	void add()
	{
		System.out.println("Have a nice day");
	}
}
	
		
public class abstracclass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		sample1 a=new sample1();
	
		a.print();
		a.add();
		

	}
}
