package muthumani;
import java.util.*;

public class collectionsexample {

	public static void main(String[] args) {
		ArrayList<String> x=new ArrayList<String>();
		x.add("a");
		x.add("x");
		x.add("a");
		x.add("m");
		x.add("n");
		//x.remove(2);
		System.out.println(x);
		x.remove(2);
		System.out.println(x);
	}

}
